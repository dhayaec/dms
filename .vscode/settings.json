{
  "files.autoSave": "afterDelay",
  "editor.fontSize": 14,
  "git.autofetch": true,
  "eslint.autoFixOnSave": true,
  "files.exclude": {
    "**/node_modules/": true,
    "**/._*": true
  },
  "cSpell.userWords": [
    "Signup",
    "feathersjs",
    "mjml",
    "onstatechange",
    "onupdatefound",
    "precached",
    "precommit",
    "promisify",
    "smtp",
    "socketio",
    "splus",
    "antialiased",
    "cellpadding",
    "cellspacing",
    "copyfiles",
    "ecmascript",
    "fastify",
    "flushall",
    "gitignore",
    "graphiql",
    "lpush",
    "lrange",
    "lspace",
    "mariadb",
    "nbsp",
    "ormconfig",
    "preheader",
    "proviced",
    "reakit",
    "rebass",
    "resave",
    "rspace",
    "typeorm",
    "varchar"
  ],
  "prettier.singleQuote": true,
  "window.zoomLevel": 0,
  "editor.formatOnSave": true,
  "tslint.autoFixOnSave": true,
  "typescript.updateImportsOnFileMove.enabled": "always"
}
